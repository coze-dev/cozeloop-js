    "@loop-infra/vitest-config": "workspace:*",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^20",
    "@types/node-fetch": "^2.x",
    "@types/promise-retry": "~1.1.6",
    "@vitest/coverage-v8": "~2.1.4",
    "axios": "^1.7.7",
    "dotenv": "~16.4.7",
    "msw": "^2.7.3",
    "rimraf": "~3.0.2",
    "tsup": "^8.0.1",
    "typescript": "^5.5.3",
    "vitest": "~2.1.4"
